@using PhotoHome.Models.Entity


@model Tag



 <div class="main-panel">
          <div class="content-wrapper">
            <div class="page-header">
              <h3 class="page-title"> Add Catagory </h3>


                    <form class="forms-sample" asp-action="AddTag" asp-controller="Tag" method="post" enctype="multipart/form-data">
                      <div class="form-group">
                        <label >Name</label>
                        <input asp-for="Name" type="text" class="form-control" id="exampleInputName1" placeholder="Name">
                        <span asp-validation-for=Name></span>
                      </div>
                      <button type="submit" class="btn btn-primary mr-2">Submit</button>

                    </form>
                  </div>
                </div>
              </div>