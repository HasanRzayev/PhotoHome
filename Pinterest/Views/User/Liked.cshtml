<main role="main" class="probootstrap-main js-probootstrap-main">
	<div class="probootstrap-bar">
		<a href="#" class="probootstrap-toggle js-probootstrap-toggle"><span class="oi oi-menu"></span></a>
		<div class="probootstrap-main-site-logo"><a href="index.html">PhotoHome</a></div>
	</div>

	<div class="container-fluid">
		<div class="d-flex flex-column align-items-center justify-content-center">
			<div class="mb-2" id="profile-picture">
				<a href="#">A</a>
			</div>
			<div class="d-flex align-items-center" id="profile-name">
				<h1 class="m-0">Adil Abbasov</h1>
			</div>
			<div class="mt-2 mb-3 d-flex align-items-center" id="profile-email">
				<h6 class="m-0">adil.abbasov@gmail.com</h6>
			</div>
			<div class="my-2" id="profile-options">
				<a href="#" class="py-2 px-3 mr-1">Share</a>
				<a href="#" class="py-2 px-3 ml-1">Edit profile</a>
			</div>
			<div class="my-5" id="profile-info">
				<a href="#" class="mr-3">Created</a>
				<a href="#" class="ml-3 info-active">Liked</a>
			</div>
		</div>
	</div>

	<div class="container-fluid">
		<div class="card-columns">
			<div class="card">
				<img class="card-img-top probootstrap-animate" src="images/accessory3.jpg" alt="Card image cap"
					 style="border-radius: 20px;">
				<div class="card-div d-flex flex-column justify-content-between align-items-end">
					<div class="pt-3 pr-3">
						<button class="card-div-btn like">
							<span style="color: #252525;"><i class="fa-solid fa-heart"></i></span>
						</button>
						<button class="card-div-btn download">
							<span style="color: #252525;"><i class="fa-solid fa-arrow-down"></i></i></span>
						</button>
					</div>
					<div class="details pb-3 pr-3">
						<button class="more">
							<span class="py-2 px-3">More</span>
						</button>
					</div>
				</div>
			</div>
		</div>
	</div>
</main>