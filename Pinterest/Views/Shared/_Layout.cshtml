<!DOCTYPE html>
<html lang="en">

<head>
	<title>PhotoHome</title>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<meta name="description" content="Free HTML5 Website Template by uicookies.com" />
	<meta name="keywords"
		  content="free bootstrap 4, free bootstrap 4 template, free website templates, free html5, free template, free website template, html5, css3, mobile first, responsive" />
	<meta name="author" content="uicookies.com" />

	<link href="https://fonts.googleapis.com/css?family=Work+Sans" rel="stylesheet">
	<link href="http://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">

	<link rel="stylesheet" href="~/css/user/bootstrap.min.css">
	<link rel="stylesheet" href="~/css/user/open-iconic-bootstrap.min.css">

	<link rel="stylesheet" href="~/css/user/owl.carousel.min.css">
	<link rel="stylesheet" href="~/css/user/owl.theme.default.min.css">

	<link rel="stylesheet" href="~/css/user/icomoon.css">
	<link rel="stylesheet" href="~/css/user/animate.css">
	<link rel="stylesheet" href="~/css/user/style.css">

	<script src="https://kit.fontawesome.com/eac357072d.js" crossorigin="anonymous"></script>
</head>

<body>
	<aside class="probootstrap-aside js-probootstrap-aside">
		<a href="#" class="probootstrap-close-menu js-probootstrap-close-menu d-md-none">
			<span><i class="fa-solid fa-arrow-left-long"></i></span> Close
		</a>
		<div class="probootstrap-site-logo probootstrap-animate" data-animate-effect="fadeInLeft">
			<a asp-action="Index" class="mb-2 probootstrap-logo">
				<img class="rounded" style="width: 120px; height: 65px; font-size: 16px !important;" src="~/images/user/logo.png" alt="">
			</a>
		</div>
		<div class="probootstrap-overflow">
			<nav class="probootstrap-nav">
				<ul>
					<li class="probootstrap-animate active pl-3" data-animate-effect="fadeInLeft">
						<a asp-action="Index">Home</a>
					</li>
					<li class="probootstrap-animate pl-3" data-animate-effect="fadeInLeft">
						<a asp-action="About">About</a>
					</li>
					<li class="probootstrap-animate pl-3" data-animate-effect="fadeInLeft">
						<a asp-action="Services">Services</a>
					</li>
					<li class="probootstrap-animate pl-3" data-animate-effect="fadeInLeft">
						<a asp-action="Portfolio">Portfolio</a>
					</li>
					<li class="probootstrap-animate pl-3" data-animate-effect="fadeInLeft">
						<a asp-action="Contact">Contact</a>
					</li>
					<li class="probootstrap-animate pl-3" data-animate-effect="fadeInLeft">
						<a asp-action="Create">Create &plus;</a>
					</li>
				</ul>
			</nav>
			<footer class="probootstrap-aside-footer probootstrap-animate" data-animate-effect="fadeInLeft">
				<ul class="list-unstyled d-flex probootstrap-aside-social">
					<li><a href="#" class="p-2"><span><i class="fa-brands fa-facebook-f"></i></span></a></li>
					<li><a href="#" class="p-2"><span><i class="fa-brands fa-instagram"></i></span></a></li>
					<li><a href="#" class="p-2"><span><i class="fa-brands fa-twitter"></i></span></a></li>
					<li><a href="#" class="p-2"><span><i class="fa-brands fa-linkedin"></i></span></a></li>
				</ul>
				<p>&copy;2022 All Rights Reserved.</p>
			</footer>
		</div>
	</aside>

	@RenderBody()

	@*<script src="~/js/user/jquery-3.2.1.slim.min.js"></script>*@
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script>
	<script src="~/js/user/popper.min.js"></script>
	<script src="~/js/user/bootstrap.min.js"></script>
	<script src="~/js/user/owl.carousel.min.js"></script>
	<script src="~/js/user/jquery.waypoints.min.js"></script>
	<script src="~/js/user/imagesloaded.pkgd.min.js"></script>
	<script src="~/js/user/main.js"></script>
	<script src="~/js/user/index.js"></script>

	@RenderSection("Scripts", required:false)

</body>

</html>